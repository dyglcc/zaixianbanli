<?xml version="1.0" encoding="UTF-8"?><LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"    xmlns:tools="http://schemas.android.com/tools"    xmlns:app1="http://schemas.android.com/apk/res/qfpay.wxshop"    android:id="@+id/lyt_shop_info"    android:layout_width="match_parent"    android:layout_height="match_parent"    android:background="@color/common_background"    android:orientation="vertical"    android:paddingLeft="5dp"    android:paddingRight="5dp" >    <include        android:layout_width="fill_parent"        android:layout_height="wrap_content"        layout="@layout/title_shop_info"        android:visibility="gone" />    <ScrollView        android:layout_width="fill_parent"        android:layout_height="fill_parent"        android:scrollbars="none" >        <LinearLayout            android:layout_width="fill_parent"            android:layout_height="wrap_content"            android:layout_marginTop="10dip"            android:orientation="vertical" >            <LinearLayout                android:id="@+id/layout_takepic"                android:layout_width="fill_parent"                android:layout_height="wrap_content"                android:orientation="horizontal" >                <TextView                    style="@style/common_text_content_big"                    android:layout_width="0px"                    android:layout_height="wrap_content"                    android:layout_gravity="center_vertical"                    android:layout_marginLeft="20dp"                    android:layout_weight="1"                    android:ellipsize="end"                    android:gravity="center_vertical|left"                    android:singleLine="true"                    android:text="@string/shopinfo_title_icon"                    android:textColor="@color/common_text_black" />                <RelativeLayout                    android:layout_width="wrap_content"                    android:layout_height="60dip"                    android:layout_marginBottom="6dp"                    android:layout_marginRight="20dip"                    android:layout_marginTop="10dp"                    android:gravity="right" >                    <qfpay.wxshop.ui.view.CustomImageView                        android:id="@+id/iv_photo"                        android:layout_width="59dip"                        android:layout_height="59dip"                        android:layout_centerInParent="true"                        android:visibility="gone"                        app1:src="@drawable/defaul_tavator" />                    <ImageView                        android:id="@+id/iv_default"                        android:layout_width="59dip"                        android:layout_height="59dip"                        android:layout_centerInParent="true"                        android:src="@drawable/defaul_tavator"                        android:visibility="visible" />                </RelativeLayout>            </LinearLayout>            <View                style="@style/form_line_horizontal"                android:layout_width="fill_parent" />            <RelativeLayout                android:id="@+id/ll_shopname_bg"                android:layout_width="fill_parent"                android:layout_height="@dimen/form_pageitem_height"                android:orientation="horizontal"                android:visibility="visible" >                <TextView                    android:id="@+id/tv_shop_name"                    style="@style/common_text_content_big"                    android:layout_width="wrap_content"                    android:layout_height="wrap_content"                    android:layout_centerVertical="true"                    android:layout_gravity="center_vertical"                    android:layout_marginLeft="20dp"                    android:clickable="false"                    android:ellipsize="end"                    android:focusable="false"                    android:focusableInTouchMode="false"                    android:gravity="center_vertical|left"                    android:singleLine="true"                    android:text="@string/shopinfo_title_name"                    android:textColor="@color/common_text_black" />                <TextView                    android:id="@+id/et_shopname"                    style="@style/common_text_content_mid"                    android:layout_width="wrap_content"                    android:layout_height="wrap_content"                    android:layout_alignParentRight="true"                    android:layout_centerVertical="true"                    android:layout_gravity="center_vertical"                    android:layout_marginRight="20dp"                    android:singleLine="true"                    android:textColor="@color/common_text_grey"                    android:textColorHint="@color/common_text_grey" />            </RelativeLayout>            <View                style="@style/form_line_horizontal"                android:layout_width="fill_parent" />            <RelativeLayout                android:id="@+id/ll_weixinhao_bg"                android:layout_width="fill_parent"                android:layout_height="@dimen/form_pageitem_height"                android:orientation="horizontal"                android:visibility="visible" >                <TextView                    android:id="@+id/tv_weixinhao"                    style="@style/common_text_content_big"                    android:layout_width="wrap_content"                    android:layout_height="wrap_content"                    android:layout_centerVertical="true"                    android:layout_marginLeft="20dp"                    android:ellipsize="end"                    android:gravity="center_vertical|left"                    android:singleLine="true"                    android:text="@string/shopinfo_title_weixin"                    android:textColor="@color/common_text_black" />                <TextView                    android:id="@+id/et_weixinhao"                    style="@style/common_text_content_mid"                    android:layout_width="wrap_content"                    android:layout_height="wrap_content"                    android:layout_alignParentRight="true"                    android:layout_centerVertical="true"                    android:layout_marginRight="20dp"                    android:singleLine="true"                    android:textColor="@color/common_text_grey"                    android:textColorHint="@color/common_line_grey" />            </RelativeLayout>            <View                style="@style/form_line_horizontal"                android:layout_width="fill_parent"                android:layout_marginBottom="20dp" />            <View                style="@style/form_line_horizontal"                android:layout_width="fill_parent" />            <RelativeLayout                android:id="@+id/ll_pickerbackground"                android:layout_width="fill_parent"                android:layout_height="@dimen/form_pageitem_height"                android:orientation="horizontal" >                <TextView                    android:id="@+id/tv_change_background_shop_title"                    style="@style/common_text_content_big"                    android:layout_width="wrap_content"                    android:layout_height="wrap_content"                    android:layout_centerVertical="true"                    android:layout_marginLeft="20dp"                    android:ellipsize="end"                    android:singleLine="true"                    android:text="@string/shopinfo_title_headerimg"                    android:textColor="@color/common_text_black" />                <TextView                    android:id="@+id/tv_shopheader"                    style="@style/common_text_content_mid"                    android:layout_width="wrap_content"                    android:layout_height="wrap_content"                    android:layout_alignParentRight="true"                    android:layout_centerVertical="true"                    android:layout_marginRight="20dp"                    android:singleLine="true"                    android:textColor="@color/common_text_grey"                    android:textColorHint="@color/common_text_grey" />            </RelativeLayout>            <View                style="@style/form_line_horizontal"                android:layout_width="fill_parent" />            <RelativeLayout                android:id="@+id/layout_dianputgonggao"                android:layout_width="fill_parent"                android:layout_height="@dimen/form_pageitem_height"                android:orientation="horizontal"                android:visibility="visible" >                <TextView                    android:id="@+id/tv_notice"                    style="@style/common_text_content_big"                    android:layout_width="wrap_content"                    android:layout_height="wrap_content"                    android:layout_centerVertical="true"                    android:layout_marginLeft="20dp"                    android:ellipsize="end"                    android:singleLine="true"                    android:text="@string/shopinfo_title_notice"                    android:textColor="@color/common_text_black" />                <TextView                    android:id="@+id/tv_text"                    style="@style/common_text_content_mid"                    android:layout_width="wrap_content"                    android:layout_height="wrap_content"                    android:layout_alignParentRight="true"                    android:layout_centerVertical="true"                    android:layout_marginRight="20dp"                    android:ellipsize="middle"                    android:hint="@string/shopinfo_text_null"                    android:maxWidth="150dp"                    android:singleLine="true"                    android:textColor="@color/common_text_grey"                    android:textColorHint="@color/common_text_grey" />            </RelativeLayout>            <View                style="@style/form_line_horizontal"                android:layout_width="fill_parent" />            <RelativeLayout                android:layout_width="fill_parent"                android:layout_height="@dimen/form_pageitem_height"                android:orientation="horizontal" >                <TextView                    style="@style/common_text_content_big"                    android:layout_width="wrap_content"                    android:layout_height="wrap_content"                    android:layout_centerVertical="true"                    android:layout_gravity="center_vertical"                    android:gravity="center_vertical|left"                    android:paddingLeft="20dp"                    android:text="@string/shopinfo_title_link"                    android:textColor="@color/common_text_black" />                <TextView                    android:id="@+id/tv_shopurl"                    style="@style/common_text_content_mid"                    android:layout_width="wrap_content"                    android:layout_height="wrap_content"                    android:layout_alignParentRight="true"                    android:layout_centerVertical="true"                    android:layout_marginRight="20dp"                    android:ellipsize="end"                    android:lines="1"                    android:textColor="@color/common_text_grey" />            </RelativeLayout>            <View                style="@style/form_line_horizontal"                android:layout_width="fill_parent" />        </LinearLayout>    </ScrollView></LinearLayout>